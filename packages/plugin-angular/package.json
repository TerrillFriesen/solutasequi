{
  "name": "@bugsnag/plugin-angular",
  "version": "6.4.3",
  "description": "Angular integration for bugsnag-js",
  "main": "dist/es5/index.js",
  "browser": "dist/es5/index.js",
  "module": "dist/esm/index.js",
  "es2015": "dist/esm/index.js",
  "esm5": "dist/es5/index.js",
  "types": "dist/esm/index.d.ts",
  "typings": "dist/esm/index.d.ts",
  "homepage": "https://www.bugsnag.com/",
  "repository": {
    "type": "git",
    "url": "git@github.com:bugsnag/bugsnag-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -fr dist && mkdir dist",
    "lint": "tslint -p .",
    "build": "npm run clean && npm run build:es2015 && npm run build:esm5",
    "build:es2015": "ngc -p tsconfig.json",
    "build:esm5": "ngc -p tsconfig.esm5.json",
    "test": "npm run lint",
    "postversion": "npm run build"
  },
  "author": "Bugsnag",
  "license": "MIT",
  "devDependencies": {
    "@angular/cli": "^7.3.9",
    "@angular/compiler": "^7.2.15",
    "@angular/compiler-cli": "^7.2.15",
    "@angular/core": "^7.2.15",
    "@bugsnag/js": "^6.4.3",
    "rxjs": "^5.5.8",
    "tslint": "^5.9.1",
    "typescript": "^3.2.4",
    "zone.js": "^0.8.26"
  }
}
